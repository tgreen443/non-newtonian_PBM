FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    bot_wall
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    side_walls
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    top_outlet
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 1e-9;
        value           uniform 1e-9;
    }
    bot_inlet
    {
        type            fixedValue;
        value           $internalField;
    }
}

