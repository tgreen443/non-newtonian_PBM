FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alphat.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bot_wall
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           $internalField;
    }
    side_walls
    {
        type            compressible::alphatWallFunction;
        Prt             0.85;
        value           $internalField;
    }
    top_outlet
    {
        type            calculated;
        value           $internalField;
    }
    bot_inlet
    {
        type            calculated;
        value           $internalField;
    }
}

