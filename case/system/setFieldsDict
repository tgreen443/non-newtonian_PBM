FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    //volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.air 1
);

regions
(
    boxToCell
    {
        box (0 0 0) (0.2 0.03 0.601);
        fieldValues
        (
            volScalarFieldValue alpha.air 0
        );
    }
);

